<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.turno.healthbeing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginSignup.ForgetPasswordSuccessMessage"></activity>
        <activity android:name=".LoginSignup.SetNewPassword" />
        <activity android:name=".LoginSignup.MakeSelection" />
        <activity android:name=".LoginSignup.ForgetPassword" />
        <activity android:name=".LoginSignup.VerifyOTP" />
        <activity android:name=".LoginSignup.SignUp" />
        <activity android:name=".LoginSignup.Login" />
        <activity android:name=".LoginSignup.StartUpScreen" />
        <activity android:name=".AllFeatures" />
        <activity android:name=".UserDashboard" />
        <activity android:name=".GoogleSignInActivity" />
        <activity android:name=".UserProfile" />
        <activity android:name=".IntroActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>